{
  "name": "nextjs-fullstack-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-dev": "husky && docker compose up -d",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "db:migrate": "tsx -r dotenv/config src/database/migrate.ts migrate",
    "db:rollback": "tsx -r dotenv/config src/database/migrate.ts rollback",
    "db:reset": "tsx -r dotenv/config src/database/migrate.ts reset",
    "db:seed": "tsx -r dotenv/config src/database/migrate.ts seed"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "hash-wasm": "^4.11.0",
    "kysely": "^0.27.4",
    "lucide-react": "^0.417.0",
    "next": "14.2.5",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.16.2",
    "uncrypto": "^0.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.3",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
